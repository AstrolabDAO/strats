{
  "name": "@astrolabs/strats",
  "version": "1.0.0",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "repository": "https://github.com/AstrolabDAO/strats",
  "author": "Astrolab DAO <contact@astrolab.fi>",
  "license": "MIT",
  "description": "Home of all of Astrolab's strategies",
  "keywords": [
    "astrolab",
    "radyal",
    "yield aggregation",
    "cross-chain",
    "swap",
    "bridge",
    "DeFi",
    "ethereum",
    "smart-contract"
  ],
  "scripts": {
    "lint": "yarn prettier --check && yarn eslint",
    "lint:fix": "yarn prettier --write && yarn eslint --fix",
    "eslint": "yarn eslint 'src/**/*.ts' 'test/**/*.ts'",
    "prettier": "yarn prettier \"**/*.{js,md,json}\"",
    "test": "yarn ts-mocha --require hardhat/register --timeout 120000 --recursive \"test/**/*.ts\" --exit",
    "build": "tsc --build .",
    "test-multisig-tenderly": "yarn hardhat test test/integration/Multisig.test.ts --network tenderly",
    "test-multisig-arbitrum": "yarn hardhat test test/integration/Multisig.test.ts --network arbitrum-mainnet-one",
    "test-aave-v3-optimizer-tenderly": "yarn hardhat test test/integration/AaveV3Optimizer.test.ts --network tenderly",
    "test-well-optimizer-tenderly": "yarn hardhat test test/integration/MoonwellOptimizer.test.ts --network tenderly",
    "test-venus-optimizer-tenderly": "yarn hardhat test test/integration/VenusOptimizer.test.ts --network tenderly",
    "test-venus-arbitrage-tenderly": "yarn hardhat test test/integration/VenusArbitrage.test.ts --network tenderly",
    "test-stargate-v1-optimizer-tenderly": "yarn hardhat test test/integration/StargateV1Optimizer.test.ts --network tenderly",
    "test-thena-amm-tenderly": "yarn hardhat test test/integration/ThenaAmm.test.ts --network tenderly",
    "test-aave-v3-optimizer-hardhat": "yarn hardhat test test/integration/AaveV3Optimizer.test.ts --network hardhat",
    "test-aave-v3-optimizer-base-mainnet": "yarn hardhat test test/integration/AaveV3Optimizer.test.ts --network base-mainnet",
    "test-aave-v3-optimizer-gnosis-mainnet": "yarn hardhat test test/integration/AaveV3Optimizer.test.ts --network gnosis-mainnet",
    "test-aave-v3-optimizer-optimism-mainnet": "yarn hardhat test test/integration/AaveV3Optimizer.test.ts --network optimism-mainnet",
    "test-aave-v3-optimizer-arbitrum-mainnet": "yarn hardhat test test/integration/AaveV3Optimizer.test.ts --network arbitrum-mainnet-one",
    "test-aave-v3-optimizer-polygon-mainnet": "yarn hardhat test test/integration/AaveV3Optimizer.test.ts --network polygon-mainnet",
    "test-well-optimizer-moonbeam-mainnet": "yarn hardhat test test/integration/MoonwellOptimizer.test.ts --network moonbeam-mainnet",
    "test-aave-v3-optimizer-bnb-chain-mainnet": "yarn hardhat test test/integration/AaveV3Optimizer.test.ts --network bnb-chain-mainnet",
    "test-venus-optimizer-bnb-chain-mainnet": "yarn hardhat test test/integration/VenusOptimizer.test.ts --network bnb-chain",
    "test-venus-arbitrage-bnb-chain-mainnet": "yarn hardhat test test/integration/VenusArbitrage.test.ts --network bnb-chain",
    "test-stargate-v1-optimizer-bnb-chain-mainnet": "yarn hardhat test test/integration/StargateV1Optimizer.test.ts --network bnb-chain",
    "test-thena-amm-bnb-chain-mainnet": "yarn hardhat test test/integration/ThenaAmm.test.ts --network bnb-chain",
    "deploy-ac-tenderly": "yarn hardhat run --network tenderly scripts/deploy-access-controller.ts",
    "deploy-ac-ethereum": "yarn hardhat run --network ethereum-mainnet scripts/deploy-access-controller.ts",
    "deploy-ac-arbitrum": "yarn hardhat run --network arbitrum-mainnet-one scripts/deploy-access-controller.ts",
    "deploy-ac-optimism": "yarn hardhat run --network optimism-mainnet scripts/deploy-access-controller.ts",
    "deploy-ac-base": "yarn hardhat run --network base-mainnet scripts/deploy-access-controller.ts",
    "deploy-ac-blast": "yarn hardhat run --network blast-mainnet scripts/deploy-access-controller.ts",
    "deploy-ac-polygon": "yarn hardhat run --network polygon-mainnet scripts/deploy-access-controller.ts",
    "deploy-ac-bnb-chain": "yarn hardhat run --network bnb-chain-mainnet scripts/deploy-access-controller.ts",
    "deploy-ac-gnosis-chain": "yarn hardhat run --network gnosis-chain-mainnet scripts/deploy-access-controller.ts",
    "deploy-ac-avalanche": "yarn hardhat run --network avalanche-mainnet scripts/deploy-access-controller.ts",
    "deploy-ac-moonbeam": "yarn hardhat run --network moonbeam-mainnet scripts/deploy-access-controller.ts",
    "deploy-ac-fantom": "yarn hardhat run --network fantom-mainnet-opera scripts/deploy-access-controller.ts",
    "deploy-ac-scroll": "yarn hardhat run --network scroll-mainnet scripts/deploy-access-controller.ts",
    "deploy-ac-linea": "yarn hardhat run --network linea-mainnet scripts/deploy-access-controller.ts",
    "deploy-ac-mantle": "yarn hardhat run --network mantle-mainnet scripts/deploy-access-controller.ts",
    "deploy-ac-mode": "yarn hardhat run --network mode-mainnet scripts/deploy-access-controller.ts",
    "deploy-ac-celo": "yarn hardhat run --network celo-mainnet scripts/deploy-access-controller.ts",
    "prepublishOnly": "yarn build",
    "clean": "yarn rimraf dist internal types *.{d.ts,js}{,.map} build-test tsconfig.tsbuildinfo"
  },
  "files": [
    "dist/",
    "README.md"
  ],
