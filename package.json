    "@types/chai": "^4.3.9",
    "@types/lodash": "^4.14.201",
    "@types/mocha": "^10.0.3",
    "@types/node": "^20.8.9",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "abitype": "^0.10.2",
    "chai": "^4.3.10",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "ethcall": "4.8.13",
    "hardhat": "^2.19.1",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-storage-layout": "^0.1.7",
    "mocha": "^10.2.0",
    "mocha-multi": "^1.1.7",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "solady": "^0.0.177",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "yarn": "^1.22.19",
    "yarn-upgrade-all": "^0.7.2"
  },
  "peerDependencies": {},
  "dependencies": {
